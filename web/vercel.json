{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json"
    }
  ],
  "routes": [
    {
      "src": "/api/instagram/refreshToken",
      "methods": ["GET"],
      "dest": "web/pages/api/instagram/refreshToken.js"
    }
  ],
  "crons": [
    {
      "path": "/api/instagram/refreshToken",
      "schedule": "0 0 */28 * *"
    }
  ]
}
